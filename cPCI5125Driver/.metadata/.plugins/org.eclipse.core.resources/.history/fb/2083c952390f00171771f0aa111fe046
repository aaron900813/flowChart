.SUFFIXES:.c .o

CC=gcc

SRCS=main.c
OBJS=$(SRCS:.c=.o)
EXEC=main
SOLIB=/home/hirain/V1.0.0/cPCI5125Driver/Lib 
SOLIBNAME=libcPCI5125.so
all: $(OBJS)
	@echo "-------------------start------------------------"
	$(CC) -o $(EXEC) $(OBJS)  -L$(SOLIB)
	@echo "--------------------OK--------------------------"

.c.o:
	$(CC) -Wall -g -o $@ -c $<

clean:
	rm -rf $(EXEC) $(OBJS)